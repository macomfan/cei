<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<sdk exchange="binance" document="https://binance-docs.github.io/apidocs/spot/cn/">

    <model name="ExchangeInfo">
        <string name="timezone"/>
		<long name="serverTime"/>
		<object_list name="rateLimits" refer="RateLimits"/>
        <object_list name="symbols" refer="Symbol"/>
    </model>
	
	<model name="RateLimits">
		<string name="rateLimitType"/>
		<string name="interval"/>
		<int name="intervalNum"/>
		<int name="limit"/>
	</model>
	
	<model name="Symbol">
		<string name="symbol"/>
		<string name="status"/>
		<string name="baseAsset"/>
		<int name="baseAssetPrecision"/>
		<string name="quoteAsset"/>
		<int name="quotePrecision"/>
		<string_list name="orderTypes"/>
		<boolean name="icebergAllowed"/>
		<boolean name="ocoAllowed"/>
	</model>


    <signature name="restful">
        <get_now output="{timestamp}" format=""/>
        <append_query_string key="AccessKeyId" value="{apikey}"/>
        <append_query_string key="SignatureMethod" value="HmacSHA256"/>
        <append_query_string key="SignatureVersion" value="2"/>
        <append_query_string key="timestamp" value="{timestamp}"/>
        <get_query_sting_list output="queryStringList"/>
        <sort_list output="{queryStringList}" input="{output}" order="ASC"/>
        <combine_string_list output="{newurlbuffer}" input="{queryStringList}" separator="&amp;"/>
        <get_method output="{method}" uppercase="true"/>
        <get_host output="{host}"/>
        <get_path output="{path}"/>
        <append_list input="{buffer}" item="{method}"/>
        <append_list input="{buffer}" item="{host}"/>
        <append_list input="{buffer}" item="{path}"/>
        <append_list input="{buffer}" item="{newurlbuffer}"/>
        <combine_string_list output="{newbuffer}" input="{buffer}" separator="\n"/>
    </signature>


    <restful client_name="MarketClient" url="https://api.binance.com">
        <interface name="getExchangeInfo">
            <headers>
                <header tag="accept-encoding" value="gzip"/>
            </headers>
            <request method="get" target="/api/v3/exchangeInfo"/>
            <response type="model">
                <json_parser model="ExchangeInfo">
                    <json_string from="timezone" to="{timezone}"/>
					<json_long from="serverTime" to="{serverTime}"/>
					<json_array from="rateLimits" to="{rateLimits}" model="RateLimits">
						<json_string from="rateLimitType" to="{rateLimitType}"/>
						<json_string from="interval" to="{interval}"/>
						<json_int from="intervalNum" to="{intervalNum}"/>
						<json_int from="limit" to="{limit}"/>
					</json_array>
					<json_array from="symbols" to="{symbols}" model="Symbol">
						<json_string from="symbol" to="{symbol}"/>
						<json_string from="status" to="{status}"/>
						<json_string from="baseAsset" to="{baseAsset}"/>
						<json_int from="baseAssetPrecision" to="{baseAssetPrecision}"/>
						<json_string from="quoteAsset" to="{quoteAsset}"/>
						<json_int from="quotePrecision" to="{quotePrecision}"/>
						<json_string_array from="orderTypes" to="{orderTypes}"/>
						<json_boolean from="icebergAllowed" to="{icebergAllowed}"/>
						<json_boolean from="ocoAllowed" to="{ocoAllowed}"/>
					</json_array>
                </json_parser>
            </response>
        </interface>

    </restful>
</sdk> 